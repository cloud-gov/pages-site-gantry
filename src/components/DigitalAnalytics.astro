---
interface Props {
  dapAgencyCode: string;
  dapSubAgencyCode?: string | null;
}

const { dapAgencyCode, dapSubAgencyCode } = Astro.props;
let dapSrc = `https://dap.digitalgov.gov/Universal-Federated-Analytics-Min.js?agency=${dapAgencyCode}`;

dapSubAgencyCode ? (dapSrc += `&subagency=${dapSubAgencyCode}`) : null;
---

<script id="_fed_an_ua_tag" src={dapSrc}></script>
